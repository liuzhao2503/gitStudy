.index_content_list {
    padding: 0 14rpx;
}

.index_content_list_z {
    display: inline-block;
    width: 50%;
}

.indexlist_wu{
    padding-top: 50rpx;
    text-align: center;
    font-size: 30rpx;
    color: #999;
}
.index_content_list_z_box {
    padding: 15rpx 10rpx 19rpx;
    background: #fff;
    margin: 10rpx 2rpx 0;
}

.index_content_list_z_box image {
    width: 100%;
    height: 254rpx;
}

.index_content_list_z_name {
    padding: 0 11rpx;
    overflow: hidden;
    font-size: 30rpx;
    line-height: 30rpx;
    margin-top: 14rpx;
    color: #595959;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.index_cpmtent_list_z_distance {
    padding: 0 11rpx;
    color: #b2b2b2;
    font-size: 26rpx;
    margin-top: 14rpx;
    height: 25rpx;
    line-height: 26rpx;
}
.index_content_list_icon-star{
    text-align: right;
    margin-right: 17rpx;
    line-height: 24rpx;
    margin-top: 18rpx;
}
.index_content_list_z .icon-star{
    line-height: 24rpx;
    font-size: 24rpx;
    margin-left: 13rpx;
    color: #c0c0c0;
}
.index_content_list_z .icon-star.active{
    color: #d33232;
}
.index_content_list_z .icon-star.index_content_starw{
    background: -webkit-linear-gradient(0deg, #d33232, #d33232 50%, #c0c0c0 0%, #c0c0c0),
                linear-gradient(90deg, #d33232, #d33232 50%, #c0c0c0 0%, #c0c0c0);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
}
