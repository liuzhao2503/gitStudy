<view class="orderlist_box" wx:if="{{index==1}}">
    <view class='orderlist_top'>
        <view class='orderlist_top_statusall'>
            <picker class='orderlist_top_status' bindchange="bindPickerChange" value="{{index}}" range="{{array}}">
                <view class="picker">
                    {{array[index]}} 
                    <image class='orderlist_top_sj' src='../../img/sanjiao.png'></image>
                </view>
            </picker>
        </view>
        <scroll-view class='orderlist_topscroll-view' scroll-x="true">
            <view class='orderlist_top_nav {{activea==0?"active":""}}' bindtap='topnavactive' data-navindex="0">全部</view>
            <view class='orderlist_top_nav {{activea==1?"active":""}}' bindtap='topnavactive' data-navindex="1">待支付</view>
            <view class='orderlist_top_nav {{activea==2?"active":""}}' bindtap='topnavactive' data-navindex="2">待消费</view>
            <view class='orderlist_top_nav {{activea==3?"active":""}}' bindtap='topnavactive' data-navindex="3">待评价</view>
            <view class='orderlist_top_nav {{activea==4?"active":""}}' bindtap='topnavactive' data-navindex="4">已取消</view>
        </scroll-view>
    </view>
    <view class='orderlist_listall'>
        <swiper current="{{activea}}" class="swiper-boxb" duration="300" style='height:100%' bindchange="bindChange">
