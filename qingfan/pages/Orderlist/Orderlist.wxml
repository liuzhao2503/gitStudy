                                <text class="top_num">订单编号：{{all_list.top_num}}</text>
                                <text class="top_status">{{the_type[all_list.top_status]}}</text>
                            </view>
                            <view class="item_middle">
                                <view class="item_middle_line">
                                    <text class="item_title">服务项目</text>
                                    <text class="item1_name">{{all_list.item1_name}}</text>
                                </view>
                                <view class="item_middle_line">
                                    <text class="item_title">预约时间</text>
                                    <text class="item2_date">{{all_list.item2_date}}</text>
                                </view>
                                <view class="item_middle_line" wx:if="{{all_list.top_status==3||all_list.top_status==4}}">
