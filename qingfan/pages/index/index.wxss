page{
    height: 100%;
}
.index_box {
    position: relative;
}
action-sheet-item{
    padding: 0;
    height:44px;
line-height:44px;
}
.index_listimgbox{
    overflow: hidden;
    height:254rpx;
}
.index_top_all{
    background: #f7f7f7;
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
    z-index: 100;
}
.index_content_list_wu{
    text-align: center;
    padding-top: 100rpx;
    font-size: 27rpx;
    color: #999;
}
.index_top {
    background: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 10rpx 0 16rpx;
}
.index_top_left{
    padding-left: 30rpx;
    
}
.index_top_left .icon-dingwei {
    display: inline-block;
    vertical-align:top;
    height: 36rpx;
    font-size: 35rpx;
    color: #ee2e64;
}

.index_address {
    /* line-height: 63rpx; */
    display: inline-block;
    /* vertical-align: middle;  */
    overflow: hidden;
    /* height: 63rpx; */
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 90rpx;
    margin-left: 5rpx;
    color: #555;
    font-size: 26rpx;
}

.index_top_middle {
    padding: 0 19rpx;
    height: 63rpx;
    line-height: 63rpx;
    background: #e9e9e9;
    border-radius: 30rpx;
    flex: 1;
    display: flex;
}

.index_top_middle input {
     height: 63rpx;
    line-height: 63rpx; 
    font-size: 26rpx;
    flex: 1;
}

.index_top_middle image {
    padding: 16rpx 10rpx 10rpx;
    display: inline-block;
    width: 32rpx;
    height: 32rpx;
}
.index_top_right{
    padding-right: 30rpx;
}
.index_top_right .icon-saoma {
    margin-left: 43rpx;
    font-size: 40rpx;
    color: #8f8f8f;
}

.index_top_left .picker {
    padding-right: 10rpx;
    height:63rpx;
    line-height:63rpx;
}

.slide-image {
    width: 100%;
    height: 100%;
}

.index_swiper {
    height: 345rpx;
    overflow: hidden;
}

.index_Check {
    position: fixed;
    right: 0;
    top: 340rpx;
    z-index: 100;
    background-color: #ff740e;
    text-align: center;
    border-top-left-radius: 24rpx;
    border-bottom-left-radius: 24rpx;
    width: 160rpx;
    height: 49rpx;
    line-height: 49rpx;
    font-size: 26rpx;
    color: #fff;
}

.index_Coupon {
    position: fixed;
    top: 50%;
    left: 0;
    z-index: 100;
    width: 45rpx;
    background-color: #ee2e64;
    border-radius: 10rpx;
    color: #fff;
    font-size: 26rpx;
    text-align: center;
    line-height: 35rpx;
    padding: 5rpx 0 10rpx;
}

.index_Coupon .icon-Coupons {
    margin-top: -10rpx;
    font-size: 30rpx;
}
.index_content{
    height: 100%;
}
.index_content_top {
    
    margin: 20rpx 26rpx 10rpx;
    border-radius: 30rpx;
    overflow: hidden;
}

.index_content_top view {
    display: inline-block;
    width: 50%;
    text-align: center;
    color: #555;
    height: 62rpx;
    font-size: 26rpx;
    background: #fff;
    line-height: 62rpx;
}

.index_content_top view.active {
    color: #fff;
    background: #ee2e64;
}

.index_content_list {
    padding: 525rpx 14rpx 0;
}

.index_content_list_z {
    display: inline-block;
    width: 50%;
}

.index_content_list_z_box {
    padding: 15rpx 10rpx 19rpx;
    background: #fff;
    margin: 10rpx 2rpx 0;
}

.index_content_list_z_box image {
    width: 100%;
    height: 254rpx;
}

.index_content_list_z_name {
    padding: 0 11rpx;
    overflow: hidden;
    font-size: 30rpx;
    line-height: 30rpx;
    margin-top: 14rpx;
    color: #595959;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.index_cpmtent_list_z_distance {
    padding: 0 11rpx;
    color: #b2b2b2;
    font-size: 26rpx;
    margin-top: 14rpx;
    height: 25rpx;
    line-height: 26rpx;
}
.index_content_list_icon-star{
    text-align: right;
    margin-right: 17rpx;
    line-height: 24rpx;
    margin-top: 18rpx;
}
.index_content_list_z .icon-star{
    line-height: 24rpx;
    font-size: 24rpx;
    margin-left: 13rpx;
    color: #c0c0c0;
}
.index_content_list_z .icon-star.active{
    color: #d33232;
}
.index_content_list_z .icon-star.index_content_starw{
    background: -webkit-linear-gradient(0deg, #d33232, #d33232 50%, #c0c0c0 0%, #c0c0c0),
                linear-gradient(90deg, #d33232, #d33232 50%, #c0c0c0 0%, #c0c0c0);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
}
