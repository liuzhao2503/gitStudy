/* pages/map/map.wxss */
page{
    height: 100%;
}
#myMap{
    width: 100%;
    height: 100%;
}
.map_bottom{
    height: 20%;
    background: #fff;
    padding: 0 50rpx;
}
.Shopname{
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
    font-size: 40rpx;
    line-height: 70rpx;
    color: #333;
}
.Shopaddress{
    font-size: 26rpx;
    line-height: 36rpx;
    color: #999;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}
.map_shoptext{
    padding-top: 50rpx;
}