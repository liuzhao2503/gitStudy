.serve_view{
	margin: 20rpx;
	width: 710rpx;
}
.serve_list_wu{
    text-align: center;
    color: #999;
    font-size: 28rpx;
    padding-top: 100rpx;
}
.serve{
	box-sizing: border-box;
	background: #fff;
	border-bottom: 1px solid #f7f7f7;
	width: 100%;
	height: 123rpx;
	position: relative;
}
.serve_photo{
	margin: 12rpx 0 0 14rpx;
	width: 120rpx;
	height: 95rpx;
	vertical-align: middle;
}
.serve_text{
	display: inline-block;
	width: 68%;
	margin-left: 47rpx;
	vertical-align: middle;
}
.serve_title{
	display: inline-block;
	width: 100%;
	font-size: 26rpx;
	color: #595959;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.serve_price{
	display: inline-block;
	width: 100%;
	font-size: 32rpx;
	color: #cd0c0c;
}
.serve_arrow{
	position: absolute;
	right: 24rpx;top: 49rpx;
	width: 14rpx;
	height: 25rpx;
}